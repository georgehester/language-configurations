{
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "[yaml]": {
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "C_Cpp.clang_format_path": "/opt/homebrew/bin/clang-format",
    "C_Cpp.clang_format_style": "file:/Users/george/.format/.clang-format",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}",
        "${workspaceFolder}/**"
    ],
    "diffEditor.wordWrap": "on",
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.autoClosingBrackets": "languageDefined",
    "editor.autoClosingComments": "languageDefined",
    "editor.autoClosingDelete": "auto",
    "editor.autoClosingOvertype": "auto",
    "editor.autoClosingQuotes": "languageDefined",
    "editor.autoIndent": "full",
    "editor.autoSurround": "languageDefined",
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": false,
    "editor.detectIndentation": false,
    "editor.find.addExtraSpaceOnTop": true,
    "editor.fontFamily": "Monaspace Neon Var",
    "editor.fontLigatures": "'ss01', 'ss03', 'ss07'",
    "editor.fontSize": 12,
    "editor.fontWeight": "600",
    "editor.formatOnSave": true,
    "editor.lineHeight": 0,
    "editor.lineNumbers": "on",
    "editor.suggest.showStatusBar": true,
    "editor.suggestSelection": "first",
    "editor.tabSize": 4,
    "editor.wordWrapColumn": 80,
    "explorer.compactFolders": false,
    "explorer.openEditors.visible": 0,
    "files.associations": {
        "*.ccjs": "javascript",
        ".clang-format": "plaintext"
    },
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "hexeditor.columnWidth": 16,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "hexeditor.showDecodedText": true,
    "java.format.settings.profile": "style",
    "java.format.settings.url": "~/.format/java-format-config.xml",
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
    "javascript.format.semicolons": "insert",
    "redhat.telemetry.enabled": false,
    "rust-analyzer.rustfmt.rangeFormatting.enable": true,
    "screencastMode.fontSize": 32,
    "screencastMode.mouseIndicatorSize": 32,
    "sql-formatter.dialect": "pl/sql",
    "sql-formatter.linesBetweenQueries": 1,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.fontFamily": "Monaco",
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
    "typescript.format.semicolons": "insert",
    "window.title": "${dirty}${rootName}${separator}${activeEditorShort}",
    "window.zoomLevel": -0.5,
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#FFD700",
        "editorBracketHighlight.foreground2": "#DA70D6",
        "editorBracketHighlight.foreground3": "#87CEFA"
    },
    "workbench.colorTheme": "Aura Dark",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.iconTheme": "eq-material-theme-icons",
    "workbench.preferredDarkColorTheme": "Atom One Dark",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "yaml.customTags": [
        "!And sequence",
        "!And",
        "!Base64",
        "!Cidr",
        "!Equals sequence",
        "!Equals",
        "!FindInMap sequence",
        "!FindInMap",
        "!GetAZs",
        "!GetAtt",
        "!If sequence",
        "!If",
        "!ImportValue sequence",
        "!ImportValue",
        "!Join sequence",
        "!Join",
        "!Not sequence",
        "!Not",
        "!Or sequence",
        "!Or",
        "!Ref",
        "!Select sequence",
        "!Select",
        "!Split sequence",
        "!Split",
        "!Sub sequence",
        "!Sub"
    ]
}
